  <h1>Edit Meal Plan for: </h1>
    <h3><%="#{@day_form.date.strftime('%A, %B %d, %Y')}" %></h3>
  <hr>
<div class="container">
  <div class="col-md-4 col-md-offset-4">
    <%= bootstrap_form_for(@day_form,
                           html: {id: 'day_form'},
                           url: day_forms_path, method: :put) do |f| %>
      <%= render partial: "form", locals: { f: f } %>
      <%= f.submit "Update Meal Plan" %>
    <% end %>
  </div>
</div>

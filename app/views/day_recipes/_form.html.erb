<%= @date %>

<%= form_for [@user, @day_recipes] do |f| %>
  <%= f.label :breakfast %>
  <%= f.text_field :recipes %>

  <%= f.label :lunch %>
  <%= f.text_field :recipes %>

  <%= f.label :dinner %>
  <%= f.text_field :recipes %>

  <%= f.label :snack %>
  <%= f.text_field :recipes %>

  <%= f.submit %>
<% end %>

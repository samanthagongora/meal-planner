<h1> Meal Plan for <br>
  <%= "#{@day.date.strftime('%A, %B %d, %Y')}" %></h1>

  <% @recipes.each do |recipe| %>
    <h3><%= recipe.name %></h3><br>
  <% end %>
  <%= link_to "Edit Meal Plan", edit_user_day_path(@day.user, @day) %>
  <%= link_to "Delete Meal Plan", user_day_path(@day.user, @day), method: :delete %>
